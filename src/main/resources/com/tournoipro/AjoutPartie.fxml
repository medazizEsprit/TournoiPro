            <ComboBox fx:id="Equipe1" layoutX="204.0" layoutY="188.0" prefWidth="150.0" promptText="Équipe 1" />
            <ComboBox fx:id="Equipe2" layoutX="204.0" layoutY="235.0" prefWidth="150.0" promptText="Équipe 2" />
